Cell[BoxData[
 RowBox[{
  RowBox[{"SurvivalInverseFourier", "[", 
   RowBox[{"NDist_", ",", "UDist_", ",", "a_", ",", "K_", ",", "M_"}], "]"}], 
  ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"SurvivalLaplaceT", ",", "Summands"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"SurvivalLaplaceT", "=", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", "s", "}"}], ",", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", 
           RowBox[{"MomentGeneratingFunction", "[", 
            RowBox[{"NDist", ",", 
             RowBox[{"Log", "[", 
              RowBox[{"MomentGeneratingFunction", "[", 
               RowBox[{"UDist", ",", 
                RowBox[{"-", "s"}]}], "]"}], "]"}]}], "]"}]}], ")"}], "/", 
         "s"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Summands", "=", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "k"}], "}"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"a", "/", "2"}], "]"}], "/", "2"}], "/", "x"}], "*", 
          RowBox[{"Re", "[", 
           RowBox[{"SurvivalLaplaceT", "[", 
            RowBox[{
             RowBox[{"a", "/", "2"}], "/", "x"}], "]"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{
           RowBox[{"Exp", "[", 
            RowBox[{"a", "/", "2"}], "]"}], "/", "x"}], "*", 
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"-", "1"}], ")"}], "^", 
              RowBox[{"(", 
               RowBox[{"i", "+", "1"}], ")"}]}], "*", 
             RowBox[{"Re", "[", 
              RowBox[{"SurvivalLaplaceT", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"a", "+", 
                   RowBox[{"I", "*", "i", "*", "2", "\[Pi]"}]}], ")"}], "/", 
                 "2"}], "/", "x"}], "]"}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "k", ",", "1"}], "}"}]}], 
           "]"}]}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Function", "[", 
      RowBox[{
       RowBox[{"{", "x", "}"}], ",", 
       RowBox[{"Sum", "[", 
        RowBox[{
